{
  "name": "promise-observer",
  "version": "1.0.12",
  "description": "An observer / event emitter implementation with promise support",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build-js && npm run build-defs",
    "build-js": "tsc",
    "build-defs": "dts-generator --name promise-observer --baseDir ./src --out d.ts/promise-observer.d.ts --main promise-observer/index ./src/index.ts",
    "test": "tape test/*.js"
  },
  "keywords": [
    "promise",
    "observer",
    "observable",
    "event",
    "events"
  ],
  "author": "spion",
  "license": "MIT",
  "devDependencies": {
    "blue-tape": "^0.1.8",
    "tsc-wrap-definition": "^1.0.4"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doxout/promise-observer.git"
  },
  "bugs": {
    "url": "https://github.com/doxout/promise-observer/issues"
  },
  "homepage": "https://github.com/doxout/promise-observer",
  "dependencies": {
    "bluebird": "^2.3.11"
  }
}
